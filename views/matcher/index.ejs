<!DOCTYPE html>
<html lang="en">
<%- include("../partials/head") %>
<body>
    <%- include("../partials/header") %>
    <main id="content">
        <div class="page">
            <% posts.forEach((post, index) => { %> 
                <div class="postCard">
                    <img class="storyPic" src="<%= post.attatchment %>">
                    <p class="petInfo">petInfo</p>
                    <img class="profilePic" src=""> 
                    <p class="userInfo"></p>
                    <p class="userStory"><%= post.content %></p>
                    <a class="actions" href="/post/view/<%= index %>">View Comments</a>
                </div>
            <% }) %> 
        </div>
    </main>
</body>
</html>